{
  "name": "fallback-stream",
  "version": "1.1.0",
  "description": "Create a readable stream that swithes to the fallback on error",
  "repository": "shinnn/fallback-stream",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/fallback-stream/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "stream",
    "read",
    "readable",
    "serial",
    "ordered",
    "fallback",
    "safe",
    "safety",
    "failsafe"
  ],
  "dependencies": {
    "multistream": "^1.4.2"
  },
  "devDependencies": {
    "eslint": "^0.11.0",
    "from2-array": "0.0.3",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.9.0",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3",
    "through2": "^0.6.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
